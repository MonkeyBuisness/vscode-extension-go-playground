{
	"name": "go-playground",
	"displayName": "go-playground",
	"description": "Simple Go Playground environment in your VScode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Programming Languages",
		"Visualization",
		"Other"
	],
	"activationEvents": [
		"onCommand:go-playground.play",
		"onView:sandboxesView",
		"onView:toysView"
	],
	"keywords": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "go-playground.play",
				"title": "Run Go Playground"
			},
			{
				"command": "sandboxesView.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "sandboxesView.newItem",
				"title": "Create New Sandbox",
				"icon": {
					"light": "resources/light/new_item.svg",
					"dark": "resources/dark/new_item.svg"
				}
			},
			{
				"command": "sandboxesView.item.open",
				"title": "Open On Playground",
				"icon": {
					"light": "resources/light/item_open.svg",
					"dark": "resources/dark/item_open.svg"
				}
			},
			{
				"command": "sandboxesView.item.delete",
				"title": "Delete sandbox",
				"icon": {
					"light": "resources/light/item_delete.svg",
					"dark": "resources/dark/item_delete.svg"
				}
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "go-playground",
					"title": "Go Playground",
					"icon": "resources/ext.svg"
				}
			]
		},
		"views": {
			"go-playground": [
				{
					"id": "toysView",
					"name": "Toys"
				},
				{
					"id": "sandboxesView",
					"name": "Sandboxes"
				}
			]
		},
		"menus": {
			"commandPalette": [
				{
					"command": "sandboxesView.refresh"
				}
			],
			"view/title": [
				{
					"command": "sandboxesView.refresh",
					"when": "view == sandboxesView",
					"group": "navigation"
				},
				{
					"command": "sandboxesView.newItem",
					"when": "view == sandboxesView",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "sandboxesView.item.delete",
					"when": "view == sandboxesView && viewItem == 'sandbox-item'",
					"group": "inline"
				},
				{
					"command": "sandboxesView.item.open",
					"when": "view == sandboxesView && viewItem == 'sandbox-item'",
					"group": "inline"
				},
				{
					"command": "sandboxesView.item.delete",
					"when": "view == sandboxesView && viewItem == 'sandbox-item'"
				},
				{
					"command": "sandboxesView.item.open",
					"when": "view == sandboxesView && viewItem == 'sandbox-item'"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"uniquefilename": "^1.1.2"
	}
}
